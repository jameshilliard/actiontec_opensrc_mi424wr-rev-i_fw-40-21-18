define_insn(spill_int, spill_Rxx)
{
	int r = sreg_int(0);
	int o = const_int(1);

	OUT(0x89);
	OUT(0x80|(r<<3)|REG_ebp);
	LOUT(o);

	debug(("movl %s,%d(ebp)\n", regname(r), o));
}

