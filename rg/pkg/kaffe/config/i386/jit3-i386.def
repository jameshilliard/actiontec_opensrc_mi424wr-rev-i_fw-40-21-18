		OUT(0x8E);
		l->at = CODEPC;
		LOUT(0);
		l->from = CODEPC;
		debug(("jle %s\n", KaffeJIT3_getLabelName(l)));
		break;
	case bgt:
		OUT(0x0F);
		OUT(0x8F);
		l->at = CODEPC;
		LOUT(0);
		l->from = CODEPC;
		debug(("jgt %s\n", KaffeJIT3_getLabelName(l)));
		break;
	case bge:
		OUT(0x0F);
		OUT(0x8D);
		l->at = CODEPC;
		LOUT(0);
		l->from = CODEPC;
		debug(("jge %s\n", KaffeJIT3_getLabelName(l)));
		break;
	case bult:
		OUT(0x0F);
		OUT(0x82);
		l->at = CODEPC;
		LOUT(0);
		l->from = CODEPC;
		debug(("jult %s\n", KaffeJIT3_getLabelName(l)));
		break;
#if 0
