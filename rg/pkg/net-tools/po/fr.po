#: ../arp.c:604
msgid ""
"Usage:\n"
"  arp [-vn]  [<HW>] [-i <if>] [-a] [<hostname>]             <-Display ARP "
"cache\n"
msgstr "Syntaxe:\n"
"  arp [-vn]  [<MAT>] [-i <if>] [-a] [<hôte>]              <-Affiche cache ARP\n"

#: ../arp.c:605
msgid ""
"  arp [-v]          [-i <if>] -d  <hostname> [pub][nopub]    <-Delete ARP "
"entry\n"
msgstr ""
"  arp [-v]          [-i <if>] -d  <hôte> [pub][nopub]     <-Supprime entrée "
"ARP\n"

