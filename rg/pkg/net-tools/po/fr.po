msgid ""
"  arp [-vnD] [<HW>] [-i <if>] -f  <filename>              <-Add entry from "
"file\n"
msgstr ""
"  arp [-vnD] [<MAT>] [-i <if>] -f  <fichier>              <-Ajout entrée depuis "
"fichier\n"

